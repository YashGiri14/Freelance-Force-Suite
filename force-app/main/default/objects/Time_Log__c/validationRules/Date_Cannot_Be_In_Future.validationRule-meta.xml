<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Cannot_Be_In_Future</fullName>
    <active>true</active>
    <errorConditionFormula>Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Date__c</errorDisplayField>
    <errorMessage>You cannot log time for a future date. Please select today&apos;s date or a past date.</errorMessage>
</ValidationRule>
